--><beans xmlns="http://www.springframework.org/schema/beans" xmlns:p="http://www.springframework.org/schema/p" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.springframework.org/schema/beans   http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

	<bean id="ExconProjectType" parent="ExconProjectType-parentBean"/>
	<bean abstract="true" id="ExconProjectType-parentBean" parent="BusinessObjectEntry">
		<property name="businessObjectClass" value="org.kuali.kra.excon.project.ExconProjectType"/>
		<property name="objectLabel" value="Export Control Project Type"/>
		<property name="inquiryDefinition">
			<ref bean="ExconProjectType-inquiryDefinition"/>
		</property>
		<property name="lookupDefinition">
			<ref bean="ExconProjectType-lookupDefinition"/>
		</property>
		<property name="titleAttribute" value="exconProjectTypeCode"/>
		<property name="attributes">
			<list>
				<ref bean="ExconProjectType-exconProjectTypeCode"/>
				<ref bean="ExconProjectType-description"/>
			</list>
		</property>
	</bean>

	<!-- Attribute Definitions -->
	
	<bean id="ExconProjectType-exconProjectTypeCode" parent="ExconProjectType-exconProjectTypeCode-parentBean"/>
	<bean abstract="true" id="ExconProjectType-exconProjectTypeCode-parentBean" parent="AttributeDefinition">
		<property name="name" value="exconProjectTypeCode"/>
		<property name="forceUppercase" value="true"/>
		<property name="label" value="Export Control Project Type Code"/>
		<property name="shortLabel" value="Export Control Project Type Code"/>
		<property name="maxLength" value="3"/>
		<property name="validationPattern">
			<bean parent="AlphaNumericValidationPattern"/>
		</property>
    <property name="control" >
    	<bean p:size="3" parent="TextControlDefinition" />
    </property>
		<property name="summary" value="Export Control Project Type Code"/>
		<property name="description" value="Export Control Project Type Code"/>
	</bean>

   <bean id="ExconProjectType-description" parent="ExconProjectType-description-parentBean" />
  <bean id="ExconProjectType-description-parentBean" abstract="true" parent="KraAttributeReferenceDummy-description"/>
       

   <bean id="ExconProjectType-versionNumber" parent="ExconProjectType-versionNumber-parentBean" />
    <bean id="ExconProjectType-versionNumber-parentBean" abstract="true" parent="AttributeReferenceDummy-versionNumber"/>

	<!-- Business Object Inquiry Definition -->
	<bean id="ExconProjectType-inquiryDefinition" parent="ExconProjectType-inquiryDefinition-parentBean"/>
	<bean abstract="true" id="ExconProjectType-inquiryDefinition-parentBean" parent="InquiryDefinition">
		<property name="title" value="Export Control Project Type Code"/>
		<property name="inquirySections">
			<list>
				<bean parent="InquirySectionDefinition">
					<property name="title" value="Export Control Project Type Code"/>
					<property name="numberOfColumns" value="1"/>
					<property name="inquiryFields">
						<list>
							<bean p:attributeName="exconProjectTypeCode" p:forceInquiry="true" parent="FieldDefinition"/>
						</list>
					</property>
				</bean>
			</list>
		</property>
	</bean>

	<!-- Business Object Lookup Definition -->
	<bean id="ExconProjectType-lookupDefinition" parent="ExconProjectType-lookupDefinition-parentBean"/>
	<bean abstract="true" id="ExconProjectType-lookupDefinition-parentBean" parent="LookupDefinition">
		<property name="title" value="Export Control Project Type Lookup"/>
		<property name="menubar" value="&lt;a href=&quot;index.jsp&quot;&gt;Main&lt;/a&gt;"/>

		<property name="defaultSort">
			<bean parent="SortDefinition">
		</bean>
		</property>
		<property name="helpDefinition" >
    	    <bean parent="HelpDefinition"
        	    p:parameterNamespace="KC-EXCON"
            	p:parameterName="exconProjectTypeMaintenanceHelp"
       	   		p:parameterDetailType="Document" />
    	</property>   				
		<property name="lookupFields">
			<list>
				<bean p:attributeName="exconProjectTypeCode" p:forceInquiry="true" parent="FieldDefinition"/>
				<bean p:attributeName="description" parent="FieldDefinition"/>
			</list>
		</property>
		<property name="resultFields">
			<list>
				<bean p:attributeName="exconProjectTypeCode" p:forceInquiry="true" parent="FieldDefinition"/>
				<bean p:attributeName="description" parent="FieldDefinition"/>
			</list>
		</property>
	</bean>
</beans>
